{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from \"@/utils/request\";\nimport Cookies from \"js-cookie\";\nexport default {\n  name: \"List\",\n  data() {\n    return {\n      admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')) : {},\n      tableData: [],\n      total: 0,\n      form: {},\n      params: {\n        pageNum: 1,\n        pageSize: 10,\n        email: '',\n        username: ''\n      }\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      request.get('admin/page', {\n        params: this.params\n      }).then(res => {\n        if (res.code === '200') {\n          this.tableData = res.data.list;\n          this.total = res.data.total;\n        }\n      });\n    },\n    reset() {\n      this.params = {\n        pageNum: 1,\n        pageSize: 10,\n        email: '',\n        username: ''\n      };\n      this.load();\n    },\n    del(email) {\n      request.delete('admin/delete/' + email).then(res => {\n        if (res.code === '200') {\n          this.$notify.success('Deleted');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    resetPass(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      // console.log(this.form)\n      request.put('admin/resetPass', this.form).then(res => {\n        if (res.code === '200') {\n          this.$notify.success('Password reset');\n          if (this.form.email === this.admin.email) {\n            Cookies.remove('admin');\n            this.$router.push('/login');\n          }\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    changeStatus(row) {\n      if (this.admin.email === row.email && !row.status) {\n        this.$notify.warning(\"Illegal Operation\");\n        row.status = true;\n      } else {\n        // console.log(row)\n        request.put('admin/update', row).then(res => {\n          if (res.code === '200') {\n            this.$notify.success('Status updated');\n            this.load();\n          } else {\n            this.$notify.error(res.msg);\n          }\n        });\n      }\n    },\n    changePageNum(pageNum) {\n      this.params.pageNum = pageNum;\n      this.load();\n    }\n  }\n};","map":{"version":3,"mappings":";AAsEA;AACA;AAEA;EACAA;EAEAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;QACAC;QACAC;QACAC;QACAC;MACA;IACA;EACA;EAEAC;IACA;EACA;EAEAC;IACAC;MACAC;QACAR;MACA;QACA;UACA;UACA;QACA;MACA;IACA;IAEAS;MACA;QACAR;QACAC;QACAC;QACAC;MACA;MACA;IACA;IAEAM;MACAF;QACA;UACA;UACA;QACA;UACA;QACA;MACA;IACA;IAEAG;MACA;MACA;MACAH;QACA;UACA;UACA;YACAI;YACA;UACA;QACA;UACA;QACA;MACA;IACA;IAEAC;MACA;QACA;QACAC;MACA;QACA;QACAN;UACA;YACA;YACA;UACA;YACA;UACA;QACA;MACA;IACA;IAEAO;MACA;MACA;IACA;EACA;AACA","names":["name","data","admin","tableData","total","form","params","pageNum","pageSize","email","username","created","methods","load","request","reset","del","resetPass","Cookies","changeStatus","row","changePageNum"],"sourceRoot":"src/views/admin","sources":["List.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- search area -->\r\n    <div style=\"margin-bottom: 2px; margin-top: 2px\">\r\n      <el-input v-model=\"params.email\" placeholder=\"Enter admin email\" style=\"width: 200px; margin-left: 2px\"></el-input>\r\n      <el-input v-model=\"params.username\" placeholder=\"Enter username\" style=\"width: 200px; margin-left: 2px\"></el-input>\r\n      <el-button type=\"primary\" style=\"margin-left: 2px; height: 40px\" icon=\"el-icon-search\" @click=\"load\">Search</el-button>\r\n      <el-button type=\"warning\" style=\"margin-left: 2px; height: 40px\" icon=\"el-icon-refresh-right\" @click=\"reset\">Reset</el-button>\r\n    </div>\r\n    <!-- table area -->\r\n    <el-table :data=\"tableData\" stripe>\r\n      <!-- status switch -->\r\n      <el-table-column label=\"Status\" width=\"100\">\r\n        <template v-slot=\"scope2\">\r\n          <el-switch\r\n              v-model=\"scope2.row.status\"\r\n              active-color=\"#13ce66\"\r\n              inactive-color=\"#ff4949\"\r\n              @change=\"changeStatus(scope2.row)\">\r\n          </el-switch>\r\n        </template>\r\n      </el-table-column>\r\n      <!-- main data -->\r\n      <el-table-column prop=\"username\" label=\"Username\" width=\"130\"></el-table-column>\r\n      <el-table-column prop=\"fname\" label=\"First Name\" width=\"130\"></el-table-column>\r\n      <el-table-column prop=\"lname\" label=\"Last Name\" width=\"130\"></el-table-column>\r\n      <el-table-column prop=\"email\" label=\"Email\" width=\"220\"></el-table-column>\r\n      <el-table-column prop=\"province\" label=\"Province/State\" width=\"150\"></el-table-column>\r\n      <el-table-column prop=\"city\" label=\"City\" width=\"150\"></el-table-column>\r\n      <el-table-column prop=\"street\" label=\"Street\" width=\"150\"></el-table-column>\r\n      <el-table-column prop=\"phone\" label=\"Phone\" with=\"80\"></el-table-column>\r\n      <!-- operations -->\r\n      <el-table-column label=\"Operations\">\r\n        <template v-slot=\"scope\">\r\n          <el-button type=\"primary\" @click=\"$router.push('/editAdmin?email=' + scope.row.email)\">Edit</el-button>\r\n          <!-- delete -->\r\n          <el-popconfirm\r\n              confirm-button-text='Yes'\r\n              cancel-button-text='No'\r\n              title=\"Are you sure you want to delete this row of data？\"\r\n              @confirm=\"del(scope.row.email)\"\r\n          >\r\n            <el-button style=\"margin-left: 2px;\" slot=\"reference\" type=\"danger\">Delete</el-button>\r\n          </el-popconfirm>\r\n          <!-- reset password -->\r\n          <el-popconfirm\r\n              confirm-button-text='Yes'\r\n              cancel-button-text='No'\r\n              title=\"Are you sure you want to reset this admin's password？\"\r\n              @confirm=\"resetPass(scope.row)\"\r\n          >\r\n            <el-button style=\"margin-left: 2px;\" slot=\"reference\" type=\"warning\">Reset</el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <!-- page -->\r\n    <el-pagination\r\n        style=\"margin-top: 5px;\"\r\n        background\r\n        :current-page=\"params.pageNum\"\r\n        :page-size=\"params.pageSize\"\r\n        @current-change=\"changePageNum\"\r\n        layout=\"prev, pager, next\"\r\n        :total=\"total\">\r\n    </el-pagination>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\nimport Cookies from \"js-cookie\";\r\n\r\nexport default {\r\n  name: \"List\",\r\n\r\n  data() {\r\n    return {\r\n      admin: Cookies.get('admin') ? JSON.parse(Cookies.get('admin')): {},\r\n      tableData: [],\r\n      total: 0,\r\n      form: {},\r\n      params: {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        email: '',\r\n        username: '',\r\n      },\r\n    }\r\n  },\r\n\r\n  created() {\r\n    this.load()\r\n  },\r\n\r\n  methods: {\r\n    load() {\r\n      request.get('admin/page', {\r\n        params: this.params\r\n      }).then(res => {\r\n        if(res.code === '200') {\r\n          this.tableData = res.data.list\r\n          this.total = res.data.total\r\n        }\r\n      })\r\n    },\r\n\r\n    reset() {\r\n      this.params = {\r\n        pageNum: 1,\r\n        pageSize: 10,\r\n        email: '',\r\n        username: '',\r\n      }\r\n      this.load()\r\n    },\r\n\r\n    del(email) {\r\n      request.delete('admin/delete/' + email).then(res => {\r\n        if(res.code === '200') {\r\n          this.$notify.success('Deleted')\r\n          this.load()\r\n        } else {\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n\r\n    resetPass(row) {\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      // console.log(this.form)\r\n      request.put('admin/resetPass', this.form).then(res => {\r\n        if(res.code === 